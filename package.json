    }
  },
  "author": "Cogworks",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^7.5.0",
    "generate-changelog": "^1.8.0",
    "husky": "^1.3.1",
    "lodash": "^4.17.21"
  },
  "dependencies": {
    "conventional-changelog-cli": "^2.0.25"
  }
}
